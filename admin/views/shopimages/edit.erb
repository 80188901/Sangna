<ul class="nav nav-tabs">
  <li> <%= link_to tag_icon(:list, pat(:list)), url(:shopimages, :index) %></li>
  <li> <%= link_to tag_icon(:plus, pat(:new)), url(:shopimages, :new) %></li>
  <li class="active"> <%= link_to tag_icon(:edit, pat(:edit)), url(:shopimages, :edit, :id => @shopimage.id) %></li>
</ul>
<div class="tabs-content">
  <% form_for :shopimage, url(:shopimages, :update, :id => @shopimage.id), :method => :put, :class => 'form-horizontal' do |f| %>
    <%= partial 'shopimages/form', :locals => { :f => f } %>
  <% end %>
</div>
